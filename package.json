{
	"name": "re-food-store",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"start:all": "docker compose up --build",
		"start:db": "docker compose up postgresql",
		"build:dashboard": "docker compose build dashboard caddy",
		"start:dashboard": "docker compose up dashboard caddy --renew-anon-volumes --build",
		"lint:prettier:fix": "npx prettier --write **/*.{ts,tsx,json}",
		"lint:stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
		"lint:eslint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
		"lint:fix:all": "concurrently \"eslint \"./src/**/*.{ts,tsx}\" --fix\" \"npx stylelint \"**/*.scss\" --fix\" \"npx prettier --write **/*.{ts,tsx,json}\"",
		"test:unit": "jest --config ./config/jest/jest.config.ts",
		"test:e2e": "cypress open",
		"test:ui": "npx loki test",
		"test:ui:approve": "npx loki approve",
		"test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
		"test:ui:json": "node ./scripts/generate-visual-json-report.js",
		"test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
		"test:ui:report": "npm run test:ui:json && npm run test:ui:html",
		"storybook": "storybook dev -p 6006 -c ./config/storybook",
		"storybook:build": "storybook build -c ./config/storybook",
		"clean": "docker compose down -v",
		"postinstall": "node ./scripts/clear-cache.js",
		"prepare": "husky install"
	},
